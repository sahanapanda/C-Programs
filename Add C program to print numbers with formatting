#include <stdio.h>

int main() {
    int N, i, j;
    scanf("%d", &N);

    for (i = 1; i <= N; i++) {
        if (i % 2 == 0) {
            printf("# ");
            for (j = 1; j < N; j++) {
                printf("%d ", i);
            }
        }
       else {
            for (j = 1; j < N; j++) {
                printf("%d ", i);
            }
            printf("*");
        }

        printf("\n");
    }

    return 0;
}








